@model IEnumerable<ERP_ZORZAL.Models.tbCliente>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Clientes</h5>
					<div class="ibox-tools">
						@*@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})*@
					</div>
                </div>
                <div class="ibox-content">

 <input type="text" placeholder="Buscar">
<table class="table table-striped">
    <tr>
        <th>
            Número Cliente
            @*@Html.DisplayNameFor(model => model.clte_Id)*@
        </th>
        <th>
            RTN/Identidad/Pasaporte
            @*@Html.DisplayNameFor(model => model.clte_RTN_Identidad_Pasaporte)*@
        </th>
   
        <th>
            @Html.DisplayNameFor(model => model.clte_Nombres)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.clte_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.clte_RTN_Identidad_Pasaporte)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.clte_Nombres)
        </td>
        
        <td>
			
            @Html.ActionLink("Seleccionar", "Create", new { id = item.clte_Id }, new { @class = "btn btn-primary btn-xs" })
      
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



