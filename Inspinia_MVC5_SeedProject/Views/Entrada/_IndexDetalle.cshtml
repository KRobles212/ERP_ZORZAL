@model IEnumerable<ERP_ZORZAL.Models.tbEntradaDetalle>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Entrada Detalle</h5>
                    @*<div class="ibox-tools">
                        <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#ModalAgregar">Nuevo</button>
                        @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
                    </div>*@
                </div>
                <div class="ibox-content">

                    <table class="table table-responsive" style="width:100%" id="tbEntradaDetalle">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.entd_Cantidad)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.entd_UsuarioCrea)
                                </th>

                                <th>
                                    @Html.DisplayNameFor(model => model.tbEntrada.ent_NumeroFormato)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
                                </th>
                                @*<th>Acciones</th>*@
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.entd_Cantidad)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.entd_UsuarioCrea)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbEntrada.ent_NumeroFormato)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
                                    </td>
                                    @*<td>
                                        @Html.ActionLink("Detalles", "Details", new { id = item.entd_Id }, new { @class = "btn btn-primary btn-sm" })
                                        @Html.ActionLink("Editar", "Edit", new { id = item.entd_Id }, new { @class = "btn btn-white btn-sm" })
                                    </td>*@
                                </tr>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/Scripts/jquery-3.1.1.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#tbEntradaDetalle').DataTable(
        {
            "searching": true,

            "oLanguage": {
                "oPaginate": {
                    "sNext": "Siguiente",
                    "sPrevious": "Anterior",
                },
                "sSearch": "Buscar",
                "sLengthMenu": "Mostrar _MENU_ registros por página",
                "sInfo": "Mostrando _START_ a _END_ Entradas",
            }
        });
    });
</script>



