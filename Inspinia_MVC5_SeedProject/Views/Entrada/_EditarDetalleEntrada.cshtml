@model ERP_ZORZAL.Models.tbEntradaDetalle




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Detalle Entrada</h5>
                </div>
                <div class="ibox-content">

				@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.entd_Id)

        <div class="form-group">
            @Html.LabelFor(model => model.ent_Id, "Tipo Entrada", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.DropDownList("ent_Id", String.Empty)
                @Html.ValidationMessageFor(model => model.ent_Id)
            </div>
            @Html.LabelFor(model => model.prod_Codigo, "Código Producto", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.DropDownList("prod_Codigo", String.Empty)
                @Html.ValidationMessageFor(model => model.prod_Codigo)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.entd_Cantidad,"Cantidad", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.entd_Cantidad)
                @Html.ValidationMessageFor(model => model.entd_Cantidad)
            </div>
            @Html.LabelFor(model => model.entd_UsuarioCrea, "Creado Por", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.entd_UsuarioCrea)
                @Html.ValidationMessageFor(model => model.entd_UsuarioCrea)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.entd_FechaCrea, "Creado en", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.entd_FechaCrea)
                @Html.ValidationMessageFor(model => model.entd_FechaCrea)
            </div>
            @Html.LabelFor(model => model.entd_UsuarioModifica, "Modificado Por", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.entd_UsuarioModifica)
                @Html.ValidationMessageFor(model => model.entd_UsuarioModifica)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.entd_FechaModifica, "Modificado en", new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                @Html.EditorFor(model => model.entd_FechaModifica)
                @Html.ValidationMessageFor(model => model.entd_FechaModifica)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Guardar" class="btn btn-primary" />
				@Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white"})
            </div>
        </div>
    </div>
}


                </div>
            </div>
        </div>
    </div>
 </div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
