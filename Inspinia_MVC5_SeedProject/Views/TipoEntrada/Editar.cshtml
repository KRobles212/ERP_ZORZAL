@model ERP_ZORZAL.Models.tbTipoEntrada

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Tipo Entrada</h2>
        @*<ol class="breadcrumb">
            <li>
                @Html.ActionLink("Listado", "Index")
            </li>
            <li class="active">
                <strong>Registrar</strong>
            </li>
        </ol>*@
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","TipoEntrada")">Regresar</a>
        </div>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Tipo Entrada</h5>
                </div>
                <div class="ibox-content">

                    <div class="form-horizontal">

                        <div class="form-group">
                            <label class="control-label col-md-2">Entrada Id</label>
                            <div class="col-md-10">
                                <input type="text" class="text-box single-line" readonly="readonly" value="1" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Descripcion</label>
                            <div class="col-md-10">
                                <input type="text" class="text-box single-line" value="Entrada Por Compra" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Usuario Crea</label>
                            <div class="col-md-10">
                                <input type="text" class="text-box single-line" value="Juan Perez" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Guardar" class="btn btn-primary" />
                                @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

