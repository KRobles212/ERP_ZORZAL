@model ERP_GMEDINA.Models.tbPuntoEmisionDetalle


    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="form-horizontal">

                        <div class="form-group col-md-5">
                            @Html.LabelFor(model => model.dfisc_Id, new { @class = "control-label col-md-4" })
                            <div class="col-md-8">
                                @Html.DropDownList("dfisc_Id", null, "Seleccione Documento Fiscal", new { @class = "form-control" })
                                <div id="validacionDocumentoFiscalCreate">
                                    <p style="color:red">@Html.ValidationMessageFor(model => model.dfisc_Id)</p>
                                </div>
                                </div>
                        </div>

                        <div class="form-group col-md-5">
                            @Html.LabelFor(model => model.pemid_RangoInicio, new { @class = "control-label col-md-4" })
                            <div class="col-md-8">
                                @Html.EditorFor(model => model.pemid_RangoInicio)
                                <div id="validacionRangoInicioCreate">
                                    <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_RangoInicio)</p>
                                </div>
                                </div>
                        </div>

                        <div class="form-group col-md-5">
                            @Html.LabelFor(model => model.pemid_RangoFinal, new { @class = "control-label col-md-4" })
                            <div class="col-md-8">
                                @Html.EditorFor(model => model.pemid_RangoFinal)
                                <div id="validacionRangoFinalCreate">
                                    <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_RangoFinal)</p>
                                </div>
                                </div>
                        </div>

                        <div class="form-group col-md-1">
                            <button class="btn btn-primary btn-xs" type="button" id="AgregarPuntoEmisionDetalle">Registrar Numeración</button>
                        </div>

                        <div class="form-group col-md-5">
                            @Html.LabelFor(model => model.pemid_FechaLimite, new { @class = "control-label col-md-4" })
                            <div class="col-md-8">
                                @Html.EditorFor(model => model.pemid_FechaLimite, "{0:MM/dd/yyyy}")
                                <div id="validacionFechaLimiteCreate">
                                    <p style="color:red">@Html.ValidationMessageFor(model => model.pemid_FechaLimite)</p>
                                </div>
                                </div>
                        </div>

                        

                    </div>
                </div>
            </div>
        </div>
    </div>
    <table id="tblPuntoEmisionDetalle" class="table table table-striped">
        <thead>
            <tr>
                <th>Documento Fiscal</th>
                <th>Rango Inicial</th>
                <th>Rango Final</th>
                <th>Fecha Límite</th>
                <th></th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>
<div id="validationSummary">
    <p style="color:red"></p>
</div>


<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/Datepicker.js"></script>
