@model IEnumerable<ERP_ZORZAL.Models.tbPuntoEmisionDetalle>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Numeración</h5>
                    
                </div>
                <div class="ibox-content">

                    <table class="table table table-striped  table-bordered">
                        <thead>
                            <tr>
                                @*<th>
                                    @Html.DisplayNameFor(model => model.tbPuntoEmision.pemi_Id)
                                </th>*@
                                <th>
                                    @Html.DisplayNameFor(model => model.tbPuntoEmision.pemi_NumeroCAI)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbDocumentoFiscal.dfisc_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.pemid_RangoInicio)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.pemid_RangoFinal)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.pemid_FechaLimite)
                                </th>
                                @*<th>
                                        @Html.DisplayNameFor(model => model.pemid_UsuarioCrea)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pemid_FechaCrea)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pemid_UsuarioModifica)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.pemid_FechaModifica)
                                    </th>*@
                                <th>Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    @*<td>
                                        @Html.DisplayFor(modelItem => item.tbPuntoEmision.pemi_Id)
                                    </td>*@
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbPuntoEmision.pemi_NumeroCAI)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbDocumentoFiscal.dfisc_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pemid_RangoInicio)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pemid_RangoFinal)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pemid_FechaLimite)
                                    </td>
                                    @*<td>
                                            @Html.DisplayFor(modelItem => item.pemid_UsuarioCrea)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.pemid_FechaCrea)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.pemid_UsuarioModifica)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.pemid_FechaModifica)
                                        </td>*@


                                    <td>

                                        <button type="button" class="btn btn-primary btn-xs" data-toggle="modal" data-target="#Detalle">
                                            Detalle
                                        </button>
                                        <button type="button" class="btn btn-white btn-xs" data-toggle="modal" data-target="#Editar_@item.pemid_Id">
                                            Editar
                                        </button>
                                        @*@Html.ActionLink("Editar", "Edit", new { id = item.pemid_Id }, new { @class = "btn btn-white btn-sm" })*@
                                        @*@Html.ActionLink("Delete", "Delete", new { id = item.pemid_Id }, new { @class = "btn btn-white btn-sm" })*@
                                    </td>
                                </tr>
                                <div class="modal fade" id="Editar_@item.pemid_Id" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Editar Numeración</h5>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                @*@Html.Partial("_EditMunicipios", new ERP_ZORZAL.Models.tbPuntoEmisionDetalle())*@
                                            </div>
                                            <div class="modal-footer">

                                                <a href="@Url.Action("Edit","PuntoEmision")" class="btn btn-primary ">Guardar</a>
                                                <a href="@Url.Action("Index","PuntoEmision")" class="btn btn-white">Cancelar</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            }
                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>



<!-- Modal -->
<!-- Modal -->
<div class="modal fade" id="Detalle" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Detalle Numeración</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @*@Html.Partial("_CreateNumeracion")*@
            </div>
            <div class="modal-footer">
                <a href="@Url.Action("Edit","PuntoEmision")" class="btn btn-primary ">Guardar</a>
                <a href="@Url.Action("Index","PuntoEmision")" class="btn btn-white">Cancelar</a>
            </div>
        </div>
    </div>
</div>



