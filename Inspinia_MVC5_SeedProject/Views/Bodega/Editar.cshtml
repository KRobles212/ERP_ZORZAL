@model ERP_ZORZAL.Models.tbBodega


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Bodegas</h2>
        @*<ol class="breadcrumb">
                <li>
                    @Html.ActionLink("List", "Index")
                </li>
                <li class="active">
                    <strong>Create</strong>
                </li>
            </ol>*@
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","Bodega")">Regresar</a>
        </div>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Bodegas</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">

                        <div class="form-group">
                            <label class="control-label col-md-1">Responsable</label>

                            <div class="col-md-4">
                                <input type="text" class="text-box single-line" value="Mirian Deras" />
                            </div>
                            <label class="control-label col-md-1">Direccion</label>

                            <div class="col-md-4">
                                <input type="text" class="text-box single-line" value="San Pedro Sula" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-1">Correo</label>

                            <div class="col-md-4">
                                <input type="text" class="text-box single-line" value="Bodega@hotmail.com" />
                            </div>
                            <label class="control-label col-md-1">Telefono</label>

                            <div class="col-md-4">
                                <input type="text" class="text-box single-line" value="96451223" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-1">Municipio</label>

                            <div class="col-md-4">
                                <select class="form-control" id="" name="">
                                    <option>Seleccione un Municipio</option>
                                    <option>San Pedro Sula </option>
                                    <option>Choloma</option>
                                    <option>La Ceiba</option>
                                    <option>Tegucigalpa</option>
                                </select>
                            </div>
                            <label class="control-label col-md-1">Estado</label>

                            <div class="col-md-4">
                                <select class="form-control" id="" name="">
                                    <option>Seleccione un Estado</option>
                                    <option>Habilitado</option>
                                    <option>Deshabilitado</option>
                                </select>
                            </div>
                        </div>




                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Guardar" class="btn btn-primary" />
                                @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                            </div>
                        </div>
                        <div class="">
                            @Html.Partial("_IndexBodegaDetalle")
                        </div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
 </div>

   

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
