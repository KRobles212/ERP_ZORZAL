@model IEnumerable<ERP_ZORZAL.Models.tbMoneda>

@{
    ViewBag.Title = "Moneda";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Monedas</h2>
        <ol class="breadcrumb">
            @*<li class="active">
                <strong>tbMoneda</strong>
            </li>*@
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Monedas</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Nuevo", "Create", null, new { @class = "btn btn-primary"})
					</div>
                </div>
                <div class="ibox-content">

<table id="DataTable" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.mnda_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mnda_Abreviatura)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.mnda_Nombre)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.mnda_FechaCrea)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.mnda_FechaModifica)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.tbUsuario.usu_NombreUsuario)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.tbUsuario1.usu_NombreUsuario)
                </th>*@
            <th>
                Acciones
            </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.mnda_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mnda_Abreviatura)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mnda_Nombre)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.mnda_FechaCrea)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.mnda_FechaModifica)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.tbUsuario.usu_NombreUsuario)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.tbUsuario1.usu_NombreUsuario)
                    </td>*@
                <td>
                    @Html.ActionLink("Detalles", "Details", new { id = item.mnda_Id }, new { @class = "btn btn-primary btn-sm" })
                    @Html.ActionLink("Editar", "Edit", new { id = item.mnda_Id }, new { @class = "btn btn-white btn-sm" })
                    @*@Html.ActionLink("Delete", "Delete", new { id=item.mnda_Id }, new { @class = "btn btn-white btn-sm"})*@
                </td>
            </tr>
        }
    </tbody>

</table>

                </div>
            </div>
        </div>
    </div>
 </div>

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>

