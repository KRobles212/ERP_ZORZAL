@model IEnumerable<ERP_ZORZAL.Models.tbDenominacion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Denominaciones</h2>
        @*@*<ol class="breadcrumb">
            <li class="active">
                <strong>tbDenominacion</strong>
            </li>
        </ol>*@
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Denominaciones</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Registrar Denominación", "Create", null, new { @class = "btn btn-primary "})
					</div>
                </div>
                <div class="ibox-content">
                    <div class="table-responsive">
                       
                            <table class="table table table-striped  table-bordered" id="DataTable" style="width:100%">
                                <thead>
                                    <tr>

                                        <th>
                                            @*@Html.DisplayNameFor(model => model.deno_Tipo)*@
                                            Moneda
                                        </th>
                                        <th>
                                            @Html.DisplayNameFor(model => model.deno_Descripcion)
                                        </th>

                                        
                                        <th>
                                            
                                            @Html.DisplayNameFor(model => model.deno_Tipo)
                                        </th>

                                        <th>
                                            @Html.DisplayNameFor(model => model.deno_valor)
                                        </th>

                                        <th>Acciones</th>
                                    </tr>

                                        @*<th>
                                        @Html.DisplayNameFor(model => model.deno_FechaCrea)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.deno_FechaModifica)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.tbUsuario.usu_NombreUsuario)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.tbUsuario1.usu_NombreUsuario)
                                    </th>*@
                                           
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.tbMoneda.mnda_Nombre)
                                            </td>

                                            <td>
                                                @Html.DisplayFor(modelItem => item.deno_Descripcion)
                                            </td>

                                            @if (item.deno_Tipo == 1)
                {
                                                <td>Billete</td>
                                            }
                                            else
                                            {
                                                <td>Moneda</td>
                                            }

                                            @*<td>
                                @Html.DisplayFor(modelItem => item.deno_Tipo)
                            </td>*@
                                            <td>
                                                @Html.DisplayFor(modelItem => item.deno_valor)
                                            </td>

                                           
                                            @*<td>
                                @Html.DisplayFor(modelItem => item.deno_FechaCrea)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.deno_FechaModifica)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.tbUsuario.usu_NombreUsuario)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.tbUsuario1.usu_NombreUsuario)
                            </td>*@
                                            
                                            <td>
                                                @Html.ActionLink("Detalle", "Details", new { id = item.deno_Id }, new { @class = "btn btn-primary btn-sm" })
                                                @Html.ActionLink("Editar", "Edit", new { id = item.deno_Id }, new { @class = "btn btn-white btn-sm" })
                                                @*@Html.ActionLink("Delete", "Delete", new { id = item.deno_Id }, new { @class = "btn btn-white btn-sm" })*@
                                            </td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
 

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>


