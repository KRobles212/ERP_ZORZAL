@model ERP_ZORZAL.Models.tbInventarioFisico

@{
    ViewBag.Title = "Detalle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Inventario Fisico</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "" })
        </div>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Detalle nventario Fisico</h5>
                    </div>
                    <div class="ibox-content">


                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.invf_Descripcion)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_Descripcion)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_ResponsableBodega)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_ResponsableBodega)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.bod_Id)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.bod_Id)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_FechaInventario)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_FechaInventario)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_UsuarioCrea)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_UsuarioCrea)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_FechaCrea)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_FechaCrea)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_UsuarioModifica)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_UsuarioModifica)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.invf_FechaModifica)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.invf_FechaModifica)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => model.tbEstadoInventarioFisico.estif_Descripcion)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.tbEstadoInventarioFisico.estif_Descripcion)
                            </dd>

                        </dl>


                        @Html.ActionLink("Edit", "Edit", new { id = Model.invf_Id }, new { @class = "btn btn-primary" })

                    </div>
                </div>
            </div>
        </div>
    </div>

