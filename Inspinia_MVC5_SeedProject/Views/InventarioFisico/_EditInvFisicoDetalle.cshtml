                            </tr>

                            <tr>
                                <td>
                                    Creado
                                </td>
                                <td>
                                    Antonio Ochoa
                                    @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                </td>
                                <td>
                                    2018/12/05
                                </td>
                             
                            </tr>
                            <tr>
                                <td>
                                    Modificado
                                </td>
                                <td>
                                    @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                                </td>
                                <td>
                                    @Html.DisplayFor(model => model.invfd_FechaModifica)
                                </td>
                               
                            </tr>

                        </table>
                        </div>

                    </div>
            </div>
        </div>
    </div>
 </div>



