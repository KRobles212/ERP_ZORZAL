@model ERP_GMEDINA.Models.tbInventarioFisicoDetalle




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Inventario Fisico Detalle</h5>
                </div>
                <div class="ibox-content">

				@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.invfd_Id)

        <div class="form-group">
            @Html.LabelFor(model => model.prod_Codigo, "Codigo Producto", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("prod_Codigo", (IEnumerable<SelectListItem>)ViewBag.prod_Codigo, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.prod_Codigo)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.prod_Codigo, "Productos", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
               @Html.DropDownList("prod_Codigo", (IEnumerable<SelectListItem>)ViewBag.prod_Descripcion, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.prod_Codigo)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.uni_Id, "Unidad de Medida", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DropDownList("uni_Id", (IEnumerable<SelectListItem>)ViewBag.uni_Id, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.uni_Id)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.invfd_CantidadSistema, new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @*@Html.EditorFor(model => model.invfd_CantidadSistema)*@
                <input value="50.00" class="form-control" />
                @Html.ValidationMessageFor(model => model.invfd_CantidadSistema)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.invfd_Cantidad,"Cantidad Fisica", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @*@Html.EditorFor(model => model.invfd_Cantidad)*@
                <input value="10.00" class="form-control" />
                @Html.ValidationMessageFor(model => model.invfd_Cantidad)
            </div>
        </div>

    </div>
}
                    @* Tabla de los datos de auditoria *@
