        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel"><label id="LblSearchByDescription">Editar</label></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12">
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Inventario Fisico</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtNombreMunicipio" value="Inventario Fisico 2018" readonly/>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Producto</label>
                            <div class="col-sm-7  input-group">
                                <select id="Select1" class="form-control">
                                    <option>Producto #1</option>
                                    <option>Producto #2</option>
                                    <option>Producto #3</option>

                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription">Cantidad</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtCodigoMunicipio" value="127" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Unidad de Medida</label>
                            <div class="col-sm-7  input-group">
                                <select id="Select1" class="form-control">
                                    <option>Unidad #1</option>
                                    <option>Unidad #2</option>
                                    <option>Unidad #3</option>

                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Creado por:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="Anthony Fugon" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Creado en:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="02/12/2018" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Modificado por:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Modificado en:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="" readonly />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <input type="button" class="btn btn-white btn-sm" data-dismiss="modal" style="width:110px;" id="btnCerrar" value="Cerrar" />
                <input type="button" class="btn btn-primary btn-sm" id="btnAceptar" style="width:110px;" value="Aceptar" onclick="Aceptar();" />
            </div>
        </div>
    </div>
</div>

