<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Inventario Fisico</h5>
                </div>
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true)
                            @Html.HiddenFor(model => model.invf_Id)
                            
                                <div class="form-group">
                                    <label class="control-label col-md-1">Descripcion</label>
                                    <div class="col-md-5">
                                        <input class="text-box single-line" value="Inventaro Fisico 2018">
                                    </div>

                                    <label class="control-label col-md-1">Responsable Bodega</label>
                                    <div class="col-md-5">
                                        <input class="text-box single-line" value="Anthony Fugon">
                                    </div>
                                </div>
                            <div class="form-group">
                                <label class="control-label col-md-1">Bodega</label>
                                <div class="col-md-5">
                                    <select id="Select1" class="form-control">
                                        <option>Bodega Central</option>
                                        <option>Bodega #1</option>
                                        <option>Bodega #2</option>
                                        <option>Bodega #3</option>
                                    </select>
                                </div>

                                <label class="control-label col-md-1">Fecha Levantamiento</label>
                                <div class="col-md-5">
                                    <input class="text-box single-line" value="02/12/2018" readonly>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-1">Estado</label>
                                <div class="col-md-5">
                                    <select id="Select1" class="form-control">
                                        <option>Activo</option>
                                        <option>Inactivo</option>
                                    </select>
                                </div>

                                <label class="control-label col-md-1">Creado por</label>
                                <div class="col-md-5">
                                    <input class="text-box single-line" value="Anthony Fugon" readonly>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-md-1">Creado el</label>
                                <div class="col-md-5">
                                    <input class="text-box single-line" value="02/12/2018" readonly>
                                </div>
                                <label class="control-label col-md-1">Modificado por</label>
                                <div class="col-md-5">
                                    <input class="text-box single-line" value="" readonly>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-md-1">Modificado el</label>
                                <div class="col-md-11">
                                    <input class="text-box single-line" value="" readonly>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-1 col-md-10">
                                    <input type="button" value="Guardar" class="btn btn-primary" />
                                    @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                                </div>
                            </div>
                        </div>


                        
                    }
                    @Html.Partial("_IndexInvFisicoDetalle_Botones", Model)

                </div>
            </div>
        </div>
    </div>
</div>


