@model IEnumerable<ERP_ZORZAL.Models.tbBanco>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Bancos</h2>
        @*<ol class="breadcrumb">
            <li class="active">
                <strong>tbBanco</strong>
            </li>
        </ol>*@
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Bancos</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Registrar", "Create", null, new { @class = "btn btn-primary btn-xs" })
					</div>
                </div>
                <div class="ibox-content">

<table class="table table table-striped  table-bordered"   id="DataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ban_Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ban_NombreContacto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ban_TelefonoContacto)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.ban_UsuarioCrea)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ban_FechaCrea)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ban_UsuarioModifica)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ban_FechaModifica)
                </th>*@
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>

@foreach (var item in Model)
    
{
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ban_Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ban_NombreContacto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ban_TelefonoContacto)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.ban_UsuarioCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ban_FechaCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ban_UsuarioModifica)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ban_FechaModifica)
                </td>*@
            <td>
                @Html.ActionLink("Detalles", "Details", new { id = item.ban_Id }, new { @class = "btn btn-primary btn-xs" })
                @Html.ActionLink("Editar", "Edit", new { id = item.ban_Id }, new { @class = "btn btn-white btn-xs" })
                @*@Html.ActionLink("Delete", "Delete", new { id=item.ban_Id }, new { @class = "btn btn-white btn-sm"})*@
            </td>
        </tr>
        }
    </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>


