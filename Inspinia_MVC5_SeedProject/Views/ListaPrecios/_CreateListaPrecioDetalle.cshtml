@model ERP_ZORZAL.Models.tbListadoPrecioDetalle


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lista PrecioDetalle </h5>
                    <div class="form-group">
                        <div class="col-md-offset-10 col-md-2">
                            <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#ModalAgregarPedidoDetalle">Agregar Lista Precio Detalle</button>


                        </div>
                    </div>
                </div>
               
                <div class="ibox-content">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()
                        <div class="form-horizontal">
                            @Html.ValidationSummary(true)

                            <div class="form-group">
                                @Html.LabelFor(model => model.listp_Id, "Número ", new { @class = "control-label col-md-2" })
                                <div class="col-md-10">
                                    @Html.DropDownList("listp_Id", null, "Seleccione Número", new { @class = "form-control" })

                                    @Html.ValidationMessageFor(model => model.listp_Id)
                                </div>
                                @*</div>

                                    <div class="form-group">*@
                                @Html.LabelFor(model => model.prod_Codigo, "Producto", new { @class = "control-label col-md-2" })
                                <div class="col-md-4">
                                    @Html.EditorFor(model => model.prod_Codigo)
                                    @*@Html.DropDownList("prod_Codigo", String.Empty)*@
                                    @Html.ValidationMessageFor(model => model.prod_Codigo)
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.lispd_PrecioMayorista, "Precio Mayorista", new { @class = "control-label col-md-2" })
                                <div class="col-md-4">
                                    @Html.EditorFor(model => model.lispd_PrecioMayorista)
                                    @Html.ValidationMessageFor(model => model.lispd_PrecioMayorista)
                                </div>
                                @*</div>

                                    <div class="form-group">*@
                                @Html.LabelFor(model => model.lispd_PrecioMinorista, "Precio Minorista", new { @class = "control-label col-md-2" })
                                <div class="col-md-4">
                                    @Html.EditorFor(model => model.lispd_PrecioMinorista)
                                    @Html.ValidationMessageFor(model => model.lispd_PrecioMinorista)
                                </div>
                            </div>




                            <div class="form-group">
                                @Html.LabelFor(model => model.lispd_DescCaja, "Descuento Caja", new { @class = "control-label col-md-2" })
                                <div class="col-md-4">
                                    @Html.EditorFor(model => model.lispd_DescCaja)
                                    @Html.ValidationMessageFor(model => model.lispd_DescCaja)
                                </div>

                                @Html.LabelFor(model => model.lispd_DescGerente, "Descuento Gerente", new { @class = "control-label col-md-2" })
                                <div class="col-md-4">
                                    @Html.EditorFor(model => model.lispd_DescGerente)
                                    @Html.ValidationMessageFor(model => model.lispd_DescGerente)
                                </div>
                            </div>

                        </div>
                        <div class="ibox-content">
                            <br />
                          
                            <div class="ibox-content">
                                <table class="table table-striped table-bordered table-hover dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>Nombre Producto</th>
                                            <th> Precio Mayorista</th>
                                            <th> Precio Minorista</th>
                                            <th>Descuento Caja</th>
                                            <th>Descuento Gerente</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="gradeX">
                                            <td></td>
                                            <td></td>
                                            <td></td>
                                            <td class="center"></td>
                                            <td class="center"></td>
                                            <td class="center"></td>
                                        </tr>

                                       
                                    </tbody>
                                </table>

                            </div>

                        </div>





                    }




                </div>
            </div>
        </div>


    </div>

</div>