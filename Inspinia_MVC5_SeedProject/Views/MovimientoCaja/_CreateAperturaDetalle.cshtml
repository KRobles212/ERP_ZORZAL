@model ERP_GMEDINA.Models.tbDenominacionArqueo


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Denominación</h5>
                </div>
                <div class="form-horizontal">
                    @Html.ValidationSummary(true)

                    <div class="form-horizontal">
                        <div class="form-group col-md-8">
                            @Html.LabelFor(model => model.deno_Id, "Denominación", new { @class = "control-label col-md-2" })
                            <div class="col-md-6">
                                @Html.DropDownList("deno_Id", null, "Seleccione Denominación", new { @class = "form-control" })
                                @Html.ValidationMessageFor(model => model.deno_Id)
                            </div>
                        </div>

                        <div class="form-group col-md-4">
                            <label class="control-label col-md-3">Cantidad</label>
                            <div class="col-md-9">
                                <input type="text" id="Cantidad" name="Cantidad" class="form-control" autocomplete="off" />
                            </div>
                        </div>
                    </div>
                </div>
                <table id="DenominacionDetalle" class="table table-striped table-bordered table-hover dataTables-example">
                    <thead>
                        <tr>
                            <th>Denominacion</th>
                            <th>Cantidad</th>
                            <th>Valor</th>
                            <th>Subtotal</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="col-md-3">
                                <label id="deno_Id" name="Cantidad"> </label>
                            </td>

                            <td class="col-md-3">
                                <label id="Cantidad" name="Cantidad"></label>
                            </td>

                            <td class="col-md-3">
                                <label type="text" id="Valor" name="Valor"></label>
                            </td>

                            <td class="col-md-3">
                                <label type="text" id="Subtotal" name="Subtotal"></label>
                            </td>

                        </tr>
                    </tbody>
                </table>
                <div id="validationSummary">
                    <p style="color:red"></p>
                </div>

            </div>
        </div>
     </div>
</div>
