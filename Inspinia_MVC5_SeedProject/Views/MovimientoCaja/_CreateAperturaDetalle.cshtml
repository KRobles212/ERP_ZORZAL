@model ERP_GMEDINA.Models.tbDenominacionArqueo


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Denominación</h5>
                </div>
                <table id="DenominacionDetalle" class="table table-striped table-bordered table-hover dataTables-example">
                    <thead>
                        <tr>
                            <th>Denominacion</th>
                            <th>Cantidad</th>
                            <th>Valor</th>
                            <th>Subtotal</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td class="col-md-3">
                                @Html.DropDownList("deno_Id", null, "Seleccione Denominación", new { @class = "form-control" })
                                <div id="validacionDenominacionCreate">
                                    <p style="color:red">@Html.ValidationMessageFor(model => model.deno_Id)</p>
                                </div>

                            </td>
                            <td class="col-md-3">
                                <input type="text" id="Cantidad" name="Cantidad" class="form-control" autocomplete="off" />
                            </td>
                            <td class="col-md-3">
                                <input type="text" id="Valor" name="Valor" class="form-control" readonly />
                            </td>
                            <td class="col-md-3">
                                <input type="text" id="Subtotal" name="Subtotal" class="form-control" readonly />
                            </td>

                        </tr>
                    </tbody>
                </table>
                <div id="validationSummary">
                    <p style="color:red"></p>
                </div>

            </div>
        </div>
     </div>
</div>
