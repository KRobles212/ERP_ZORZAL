@model ERP_ZORZAL.Models.tbSolicitudEfectivoDetalle



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                @*<div class="ibox-title">
                    <h5>Detalle de Solicitud Efectivo</h5>
                </div>*@
                <div class="ibox-content">

				@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true)






        <div class="form-group">
            @Html.LabelFor(model => model.solef_Id, "Número", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @*@Html.DropDownList("solef_Id", String.Empty)*@
                @Html.EditorFor(model => model.soled_Id, new { htmlAttributes = new { disabled = true } })
                @Html.ValidationMessageFor(model => model.solef_Id)
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.solef_Id, "Moneda", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @*@Html.DropDownList("solef_Id", String.Empty)*@
                @Html.EditorFor(model => model.tbSolicitudEfectivo.mnda_Id, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
               
            </div>
        </div>



        <div class="form-group">
            <label class="control-label col-md-3">Denominación</label>
            <div class="col-md-9">

                @Html.DropDownListFor(x => x.soled_Id, new List<SelectListItem>
    {
                        new SelectListItem() {Text = "500", Value="1"},
                        new SelectListItem() {Text = "100", Value="2"},

    }, "Seleccione la Denominación", new { @class = "form-control" })


            </div>
        </div>







        <div class="form-group">
            <label class="control-label col-md-2">Cantidad Solicitada</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.soled_CantidadSolicitada)
                @Html.ValidationMessageFor(model => model.soled_CantidadSolicitada)
            </div>
        </div>

       

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Agregar" class="btn btn-primary" />
				@Html.ActionLink("Cancelar", "Create", null, new { @class = "btn btn-white"})
            </div>
        </div>
    </div>
}

                </div>
            </div>
        </div>
    </div>
 </div>

 

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
