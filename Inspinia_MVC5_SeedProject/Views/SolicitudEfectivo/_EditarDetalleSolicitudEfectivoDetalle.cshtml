@model ERP_ZORZAL.Models.tbSolicitudEfectivoDetalle



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar Detalle</h5>
                </div>
                <div class="ibox-content">

				@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.soled_Id)

        <div class="form-group">
            <label class="control-label col-md-2">Número</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.tbSolicitudEfectivo.solef_Id , new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                @Html.ValidationMessageFor(model => model.solef_Id )
            </div>
        </div>



        <div class="form-group">
            @Html.LabelFor(model => model.deno_Id, "Denominación", new { @class = "control-label col-md-3" })
            <div class="col-md-9">
                @Html.EditorFor(model => model.tbDenominacion.deno_Descripcion, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                @Html.ValidationMessageFor(model => model.deno_Id)
            </div>
        </div>
        
        

        <div class="form-group">
            @Html.LabelFor(model => model.soled_CantidadSolicitada,"Cantidad Solicitada", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.soled_CantidadSolicitada )
                @Html.ValidationMessageFor(model => model.soled_CantidadSolicitada)
            </div>
        </div>




        <div class="form-group">
            @Html.LabelFor(model => model.soled_CantidadEntregada,"Cantidad Entregada", new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.soled_CantidadEntregada )
                @Html.ValidationMessageFor(model => model.soled_CantidadEntregada)
            </div>
        </div>

       

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Guardar" class="btn btn-primary"  />
				@Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-white"})
            </div>
        </div>
    </div>
}


                </div>
            </div>
        </div>
    </div>
 </div>


