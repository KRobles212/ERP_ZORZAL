@model ERP_ZORZAL.Models.tbCliente

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="~/Content/css/Auditoria.css" rel="stylesheet" />


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Detalles Cliente</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            <a href="@Url.Action("Index","Cliente")">Regresar</a>
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Cliente</h5>
                </div>
                <div class="ibox-content">


                    <div class="row">
                        <div class="row">
                            <div class="col-md-4" id="natural" name="natural">
                                <dl class="dl-horizontal">

                                    <dt>
                                        @Html.DisplayNameFor(model => model.clte_Identificacion)
                                    </dt>
                                    <dd>
                                        @Html.DisplayFor(model => model.clte_Identificacion)
                                    </dd>
                                    @if (Model.clte_EsPersonaNatural)
                                    {
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Nombres)
                                        </dt>
                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Nombres)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Apellidos)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Apellidos)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_FechaNacimiento)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_FechaNacimiento)
                                        </dd>
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Sexo)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Sexo)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Telefono)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Telefono)
                                        </dd>
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Direccion)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Direccion)
                                        </dd>
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_CorreoElectronico)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_CorreoElectronico)
                                        </dd>
                                    }
                                    else
                                    {
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_Nacionalidad)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_Nacionalidad)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_NombreComercial)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_NombreComercial)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_RazonSocial)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_RazonSocial)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_ContactoNombre)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_ContactoNombre)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_ContactoEmail)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_ContactoEmail)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_ContactoTelefono)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_ContactoTelefono)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_FechaConstitucion)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_FechaConstitucion)
                                        </dd>
                                    }
                                                           
                                    <dt>
                                        @Html.DisplayNameFor(model => model.clte_EsActivo)
                                    </dt>
                                    @if (Model.clte_EsActivo)
                                    {
                                        <dd>Sí</dd>
                                    }
                                    else
                                    {
                                        <dd>No</dd>
                                        <dt>
                                            @Html.DisplayNameFor(model => model.clte_RazonInactivo)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.clte_RazonInactivo)
                                        </dd>
                                    }
                                    <dt>
                                        @Html.DisplayNameFor(model => model.clte_ConCredito)
                                    </dt>

                                    @if (Model.clte_ConCredito)
                                    {
                                        <dd>Sí</dd>
                                    }
                                    else
                                    {
                                        <dd>No</dd>
                                    }

                                    <dt>
                                        @Html.DisplayNameFor(model => model.clte_EsMinorista)
                                    </dt>
                                    @if (Model.clte_EsMinorista)
                                    {
                                        <dd>Sí</dd>
                                    }
                                    else
                                    {
                                        <dd>No</dd>
                                    }
                                    <dt>
                                        Municipio
                                    </dt>
                                    <dd>
                                        @Html.DisplayFor(model => model.tbMunicipio.mun_Nombre)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.clte_Observaciones_)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.clte_Observaciones_)
                                    </dd>
                                </dl>  
                            </div>
                        </div>
                        @Html.ActionLink("Editar", "Edit", new { id = Model.clte_Id }, new { @class = "btn btn-primary" })
                    </div>

                </div>
            </div>
            <div class="ibox-content">
                <table class="table table-striped">
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                    <tr>
                        <td>Creado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuario.usu_Nombres)</td>
                        <td>@Html.DisplayFor(model => model.clte_FechaCrea)</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        <td>@Html.DisplayFor(model => model.tbUsuario1.usu_Nombres)</td>
                        <td>@Html.DisplayFor(model => model.clte_FechaModifica)</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/PersonaNatural.js"></script>
<script src="~/Scripts/app/Activo.js"></script>













