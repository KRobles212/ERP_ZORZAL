@model ERP_GMEDINA.Models.tbProductoCategoria

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/css/Auditoria.css" rel="stylesheet" />
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Producto Categoría</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Producto Categoría</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.pcat_Id)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.pcat_Id)
                            </dd>
                            <dt>
                                @Html.DisplayNameFor(model => model.pcat_Nombre)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => model.pcat_Nombre)
                            </dd>
                        </dl>
                        @Html.ActionLink("Editar", "Edit", new { id = Model.pcat_Id }, new { @class = "btn btn-primary" })
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h5>Listado de SubCategoría</h5>
        </div>
        <div class="ibox-content">
            <div class="form-horizontal">
                @Html.Partial("_IndexSubCategoria", Model.tbProductoSubcategoria)
            </div>
        </div>
    </div>

    <div class="ibox-content">
        <table class="table table table-striped  table-bordered">
            <tr>
                <th>
                    Acción
                </th>
                <th>
                    Usuario
                </th>
                <th>
                    Fecha
                </th>
            </tr>
            <tr>
                <td>
                    Creado
                </td>
                <td>
                    @Html.DisplayFor(model => model.pcat_UsuarioCrea)
                </td>
                <td>
                    @Html.DisplayFor(model => model.pcat_FechaCrea)
                </td>
            </tr>
            <tr>
                <td>
                    Modificado
                </td>
                <td>
                    @Html.DisplayFor(model => model.pcat_UsuarioModifica)
                </td>
                <td>
                    @Html.DisplayFor(model => model.pcat_FechaModifica)
                </td>
            </tr>
        </table>
    </div>

</div>
<script src="~/Scripts/app/BusquedaGenerica.js"></script>
<script src="~/Scripts/app/DataTables.js"></script>









