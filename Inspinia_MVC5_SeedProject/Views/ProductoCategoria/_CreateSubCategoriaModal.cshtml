@model ERP_GMEDINA.Models.tbProductoSubcategoria
@using ERP_GMEDINA.Models
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox-content">


                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()


                    <div class="form-horizontal">
                        @Html.ValidationSummary(true)

                        <div class="form-group">
                            @Html.LabelFor(model => model.pscat_Id, new { @class = "control-label col-md-2" })
                            <div class="col-md-4">
                                <input class="form-control" value="" readonly />
                                @Html.ValidationMessageFor(model => model.pscat_Id)

                            </div>
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(model => model.pscat_Descripcion, new { @class = "control-label col-md-2" })
                            <div class="col-md-4">
                                @Html.EditorFor(model => model.pscat_Descripcion)
                                @Html.ValidationMessageFor(model => model.pscat_Descripcion)
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-4">
                            </div>
                            <button class="btn btn-primary" type="button" id="AgregarSubCategorias">Añadir</button>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
