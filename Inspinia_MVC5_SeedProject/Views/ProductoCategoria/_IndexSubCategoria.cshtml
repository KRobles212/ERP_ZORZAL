@model IEnumerable<ERP_ZORZAL.Models.tbProductoSubcategoria>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-content">

                    <table class="table table table-striped  table-bordered" id="DataTable">
                        <thead>
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.pscat_Id)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.pscat_Descripcion)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.tbProductoCategoria.pcat_Nombre)
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pscat_Id)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.pscat_Descripcion)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.tbProductoCategoria.pcat_Nombre)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>


                </div>
            </div>
        </div>
    </div>
 </div>

<script src="~/Scripts/app/BusquedaGenerica.js"></script>
<script src="~/Scripts/jquery-3.3.1.js"></script>
