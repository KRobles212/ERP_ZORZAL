@model ERP_ZORZAL.Models.tbDepartamento

@{
    ViewBag.Title = "Detalle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Departamento</h2>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index")
        </div>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Departamento</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.dep_Codigo)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.dep_Codigo)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.dep_Nombre)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.dep_Nombre)
                        </dd>

                    </dl>  
                </div>
            </div>


            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Municipios</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        @Html.Partial("_IndexMunicipio", Model.tbMunicipio)
                        <div class="form-group">
                            <div class="col-md-12 text-center">
                                @Html.ActionLink("Editar", "Edit", new { id = 5 }, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>



        <div class="ibox-content">
            <table class="table table-striped">
                <tr>
                    <th>
                        Acción
                    </th>
                    <th>
                        Usuario
                    </th>
                    <th>
                        Fecha
                    </th>
                </tr>
                <tr>
                    <td>
                        Creado por
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.dep_FechaCrea)
                    </td>
                </tr>
                <tr>
                    <td>
                        Modificado por
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.tbUsuario.usu_Nombres)
                    </td>
                    <td>
                        @Html.DisplayFor(model => model.dep_FechaModifica)
                    </td>

                </tr>

            </table>
        </div>
     </div>
  </div>












