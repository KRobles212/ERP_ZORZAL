@model ERP_GMEDINA.Models.tbMunicipio


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">


                <div class="form-horizontal">
                   

                    @Html.HiddenFor(model => model.dep_Codigo)

                    <input type="hidden" name="munCodigoCreate" id="munCodigoCreate_@Model.mun_Codigo" value="@Model.mun_Codigo" />
                    <input type="hidden" name="munNombreCreate" id="munNombreCreate_@Model.mun_Codigo" value="@Model.mun_Nombre" />
                    <input type="hidden" name="depCodigoCreate" id="depCodigoCreate_@Model.mun_Codigo" value="@Model.dep_Codigo" />



                <div class="form-group">
                    @Html.LabelFor(model => model.mun_Codigo, new { @class = "control-label col-md-2" })
                    <div class="col-md-7">
                        @{string munCodigo = "munCodigoCreate_" + @Model.mun_Codigo;}
                        @Html.EditorFor(model => model.mun_Codigo, new { @class = "form-control", id = @munCodigo })
                        <div id="validationmunCodigoCreate_@Model.mun_Codigo">
                            @Html.ValidationMessageFor(model => model.mun_Codigo)
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.mun_Nombre, new { @class = "control-label col-md-2" })
                    <div class="col-md-7">
                        @{string munNombre = "munNombreCreate_" + @Model.mun_Codigo;}
                        @Html.EditorFor(model => model.mun_Nombre, new { @class = "form-control", id = @munNombre })
                        <div id="validationmunNombreCreate_@Model.mun_Codigo">
                            @Html.ValidationMessageFor(model => model.mun_Nombre)
                        </div>
                    </div>
                    <div class="form-group col-md-1">
                        <button class="btn btn-primary" type="button" id="AgregarMunicipios">Añadir</button>
                    </div>
                </div>

                    <table id="tblMunicipios" class="table table table-striped">
                        <thead>
                            <tr>
                                <th>Código Municipio</th>
                                <th>Municipio</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}