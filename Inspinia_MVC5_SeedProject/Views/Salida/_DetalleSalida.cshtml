@model ERP_GMEDINA.Models.tbSalidaDetalle



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Salida Detalle</h5>
                </div>
                <div class="ibox-content">


                    <dl class="dl-horizontal">
                        <dt>
                            @Html.DisplayNameFor(model => model.sal_Cantidad)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.sal_Cantidad)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tbProducto.prod_Descripcion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.tbSalida.box_Codigo)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.tbSalida.box_Codigo)
                        </dd>

                    </dl>



                    @Html.ActionLink("Editar", "Edit", new { id = Model.sal_Id }, new { @class = "btn btn-primary" })
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>

                </div>
            </div>
        </div>
    </div>
</div>



