@model IEnumerable<ERP_ZORZAL.Models.tbCaja>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Caja</h2>
        <ol class="breadcrumb">
            <li class="active">
                <strong></strong>
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lista de Caja</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Nuevo", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped table-responsive" style="width:100%" id="DataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.suc_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cja_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.cja_Descripcion)
            </th>
            @*<th>
                    @Html.DisplayNameFor(model => model.cja_UsuarioCrea)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.cja_FechaCrea)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.cja_UsuarioModifica)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.cja_FechaModifica)
                </th>*@
            <th></th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.suc_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cja_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.cja_Descripcion)
                </td>
                @*<td>
                        @Html.DisplayFor(modelItem => item.cja_UsuarioCrea)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.cja_FechaCrea)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.cja_UsuarioModifica)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.cja_FechaModifica)
                    </td>*@
                <td>
                    @Html.ActionLink("Detalles", "Details", new { id = item.cja_Id }, new { @class = "btn btn-primary btn-sm" })
                    @Html.ActionLink("Editar", "Edit", new { id = item.cja_Id }, new { @class = "btn btn-white btn-sm" })
                    @*@Html.ActionLink("Delete", "Delete", new { id=item.cja_Id }, new { @class = "btn btn-white btn-sm"})*@
                </td>
            </tr>
        }
    </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>
<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>


