@model IEnumerable<ERP_ZORZAL.Models.tbPuntoEmisionDetalle>

<!-- Boton Modal Create Punto Emision Detalle -->
<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#CreatePuntoEmisionDetalle">
    Nuevo
</button>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Punto Emisión Detalle</h5>
					@*<div class="ibox-tools">
						@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>*@
                </div>
                <div class="ibox-content">

<table class="table table-striped table-bordered" id="DataTable">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.dfisc_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_RangoInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_RangoFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_FechaLimite)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.pemid_UsuarioCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_FechaCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_UsuarioModifica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pemid_FechaModifica)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.tbPuntoEmision.pemi_NumeroCAI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tbPuntoEmision1.pemi_NumeroCAI)
            </th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.dfisc_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_RangoInicio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_RangoFinal)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_FechaLimite)
                </td>
                @*<td>
                    @Html.DisplayFor(modelItem => item.pemid_UsuarioCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_FechaCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_UsuarioModifica)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.pemid_FechaModifica)
                </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.tbPuntoEmision.pemi_NumeroCAI)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.tbPuntoEmision1.pemi_NumeroCAI)
                </td>
                <td>
                    @*@Html.ActionLink("Detalle", "Details", new { id = item.pemid_Id }, new { @class = "btn btn-primary btn-sm" })*@
                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#ModalEditarPED">
                        Editar
                    </button>
                    @*@Html.ActionLink("Edit", "Edit", new { id = item.pemid_Id }, new { @class = "btn btn-white btn-sm" })
                    @Html.ActionLink("Delete", "Delete", new { id = item.pemid_Id }, new { @class = "btn btn-white btn-sm" })*@
                </td>
            </tr>
        }

    </tbody>
   

</table>

                </div>
            </div>
        </div>
    </div>
 </div>


<script src="~/Scripts/jquery-3.3.1.js"></script>
<script src="~/Scripts/app/DataTable.js"></script>

<!-- Modal Create Punto Emision Detalle -->
<div class="modal fade" id="CreatePuntoEmisionDetalle" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Ingresar Punto Emision Detalle</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @Html.Partial("_CreatePuntoEmisionDetalle")
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary">Guardar</button>
            </div>
        </div>
    </div>
</div>


<!-- Modal Editar Punto Emision Detalle -->
<div class="modal fade" id="ModalEditarPED" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Editar Punto Emision Detalle</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @Html.Partial("_EditarPuntoEmisionDetalle")
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary">Guardar</button>
            </div>
        </div>
    </div>
</div>
